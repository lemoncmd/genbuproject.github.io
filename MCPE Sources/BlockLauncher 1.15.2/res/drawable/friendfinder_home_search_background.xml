<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="0x106000d" />
            <stroke android:width="2dp" android:color="@color/white_10_percent" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <solid android:color="0x106000d" />
            <stroke android:width="2dp" android:color="@color/white_10_percent" />
        </shape>
    </item>
    <item android:drawable="@color/white_10_percent" />
</selector>
